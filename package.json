    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/websockets": "^10.2.6",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "he": "^1.2.0",
    "ioredis": "^5.3.2",
    "mongoose": "^7.5.3",
